{
	"name": "@mhmo91/schmancy",
	"version": "0.0.212",
	"description": "UI library build with web components",
	"main": "./dist/index.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/mhmo91/schmancy.git"
	},
	"keywords": [
		"lit",
		"UI",
		"Schmancy"
	],
	"type": "module",
	"types": "./types/src/index.d.ts",
	"module": "./dist/index.js",
	"files": [
		"dist",
		"types",
		"README.md"
	],
	"publishConfig": {
		"@mhmo91:registry": "https://npm.pkg.github.com"
	},
	"scripts": {
		"dev": "vite --config demo/vite.config.ts --host",
		"dev2": "vite --config demo2/vite.config.ts --host",
		"build:demo": "vite build --config demo/vite.config.ts",
		"build": "tsc && vite build --config vite.config.ts",
		"preview": "vite preview",
		"batch": "npm version patch",
		"prepare": "husky install",
		"build:components": "wca ./src --format json --outFile ./hummingbird/src/toolkit/components.json",
		"watch": "npm-watch build:components"
	},
	"author": "@mhmo91",
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/mhmo91/schmancy/issues"
	},
	"homepage": "https://github.com/mhmo91/schmancy#readme",
	"dependencies": {
		"@juliangarnierorg/anime-beta": "^4.0.0-beta.98",
		"@lit-labs/motion": "^1.0.3",
		"@lit/context": "^1.1.0",
		"@material/material-color-utilities": "^0.2.7",
		"@mhmo91/lit-mixins": "latest",
		"@rollup/plugin-strip": "^3.0.4",
		"@rollup/plugin-terser": "^0.4.4",
		"@types/cleave.js": "^1.4.12",
		"cleave.js": "^1.6.0",
		"lit": "^3.1.4",
		"moment": "^2.30.1",
		"rxjs": "latest",
		"tippy.js": "^6.3.7",
		"ts-is-present": "^1.2.2",
		"typescript": "^5.3.3",
		"uuid": "^9.0.0",
		"web-component-analyzer": "^1.1.7"
	},
	"devDependencies": {
		"@tailwindcss/container-queries": "^0.1.1",
		"@tailwindcss/forms": "^0.5.7",
		"@types/node": "^20.3.1",
		"@types/uuid": "^9.0.2",
		"@typescript-eslint/parser": "^5.60.0",
		"autoprefixer": "^10.4.14",
		"deepmerge": "^4.3.1",
		"eslint": "^8.49.0",
		"husky": "^8.0.3",
		"postcss": "^8.4.32",
		"prettier": "^3.0.3",
		"sass": "^1.62.1",
		"tailwindcss": "^3.3.6",
		"vite": "^5.0.7",
		"vite-plugin-sitemap": "^0.4.2",
		"vite-plugin-webfont-dl": "^3.7.4"
	}
}
