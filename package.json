{
	"name": "@mhmo91/schmancy",
	"version": "0.0.251",
	"description": "UI library build with web components",
	"main": "./dist/index.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/mhmo91/schmancy.git"
	},
	"keywords": [
		"lit",
		"UI",
		"Schmancy"
	],
	"type": "module",
	"types": "./types/src/index.d.ts",
	"module": "./dist/index.js",
	"files": [
		"dist",
		"types",
		"README.md"
	],
	"publishConfig": {
		"@mhmo91:registry": "https://npm.pkg.github.com"
	},
	"scripts": {
		"dev": "vite --config demo/vite.config.ts --host",
		"dev2": "vite --config demo2/vite.config.ts --host",
		"build:demo": "vite build --config demo/vite.config.ts",
		"build": "tsc && vite build --config vite.config.ts",
		"preview": "vite preview",
		"batch": "npm version patch",
		"prepare": "husky install",
		"build:components": "wca ./src --format json --outFile ./hummingbird/src/toolkit/components.json",
		"watch": "npm-watch build:components"
	},
	"author": "@mhmo91",
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/mhmo91/schmancy/issues"
	},
	"homepage": "https://github.com/mhmo91/schmancy#readme",
	"dependencies": {
		"@floating-ui/dom": "^1.6.10",
		"@juliangarnierorg/anime-beta": "^4.0.0-beta.100",
		"@lit-labs/motion": "^1.0.7",
		"@lit-labs/virtualizer": "^2.0.14",
		"@lit/context": "^1.1.2",
		"@material/material-color-utilities": "^0.3.0",
		"@material/web": "^2.1.0",
		"@mhmo91/lit-mixins": "^0.3.9",
		"@rollup/plugin-strip": "^3.0.4",
		"@rollup/plugin-terser": "^0.4.4",
		"@types/cleave.js": "^1.4.12",
		"cleave.js": "^1.6.0",
		"fastest-levenshtein": "^1.0.16",
		"lit": "^3.2.0",
		"moment": "^2.30.1",
		"rxjs": "latest",
		"tippy.js": "^6.3.7",
		"ts-is-present": "^1.2.2",
		"typescript": "^5.5.4",
		"uuid": "^10.0.0",
		"web-component-analyzer": "^2.0.0"
	},
	"devDependencies": {
		"@tailwindcss/container-queries": "^0.1.1",
		"@tailwindcss/forms": "^0.5.8",
		"@types/node": "^22.5.2",
		"@types/uuid": "^10.0.0",
		"@typescript-eslint/parser": "^8.4.0",
		"autoprefixer": "^10.4.20",
		"deepmerge": "^4.3.1",
		"eslint": "^9.9.1",
		"husky": "^9.1.5",
		"postcss": "^8.4.44",
		"prettier": "^3.3.3",
		"sass": "^1.77.8",
		"tailwindcss": "^3.4.10",
		"vite": "^5.4.2",
		"vite-plugin-sitemap": "^0.7.1",
		"vite-plugin-webfont-dl": "^3.9.5"
	}
}
